{
  "manifest_version": 3,
  "name": "Request Interceptor",
  "version": "1.0",
  "description": "Intercepts and modifies HTTP requests",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "webRequest",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
